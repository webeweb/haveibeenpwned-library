{
    "name": "webeweb/haveibeenpwned-library",
    "description": "Integrate HaveIBeenPwned API with your projects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "webeweb",
        "library",
        "haveibeenpwned"
    ],
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "homepage": "https://github.com/webeweb/haveibeenpwned-library",
    "support": {
        "issues": "https://github.com/webeweb/haveibeenpwned-library/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "webeweb/core-library": "^9.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\HaveIBeenPwned\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\HaveIBeenPwned\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
