{
    "name": "webeweb/haveibeenpwned-library",
    "type": "library",
    "description": "Integrate HaveIBeenPwned API with your projects",
    "keywords": [
        "API",
        "haveibeenpwned",
        "library",
        "password",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/haveibeenpwned-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0",
        "webeweb/core-library": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\HaveIBeenPwned\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\HaveIBeenPwned\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/haveibeenpwned-library/issues"
    }
}
